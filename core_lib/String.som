"Implementation of String class"

String = (

    concatenate: other = primitive
    + other = ( ^self concatenate: other asString )
    
    "Converting"
    asString = ( ^self )
    
    print = primitive
    printLn = primitive

    "Info"
    length = primitive
    isWhitespace = primitive
    isLetters = primitive
    isDigits = primitive

    asInteger = (
        ^ Integer fromString: self
    )

    "Comparation"
    = other = primitive

)